{
    "name": "codewiser/laravel-notifications",
    "description": "Laravel Notification helpers",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Codewiser\\Notifications\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "pm",
            "email": "pm@101media.ru"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0|^11.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codewiser\\Notifications\\NotificationsServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.1"
    }
}
